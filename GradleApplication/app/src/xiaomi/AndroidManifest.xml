<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gradle">

    <application
        android:allowBackup="true"
        android:icon="${icon}"
        android:label="${app_name}"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name="com.example.administrator.gamelistlibrary.YppMainActivity"/>-->
        <activity android:name="com.gradle.mylibrary2.Library2Activity" android:exported="true"/>
        <activity android:name="com.gradle.mylibrary.LibraryActivity" android:exported="true"/>
        <meta-data
            android:name="app_name"
            android:value="${app_name}"/>

        <meta-data
            android:name="icon"
            android:value="${icon}"/>

        <meta-data
            android:name="welcome_bg"
            android:value="${welcome_bg}"/>

        <meta-data
            android:name="tint_color"
            android:value="${tint_color}"/>

        <meta-data
            android:name="load_url"
            android:value="${load_url}"/>

    </application>

</manifest>